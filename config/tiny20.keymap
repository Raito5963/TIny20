&kp AT  &kp HASH  &kp PLUS  &kp EQUAL  &kp LPAR  &kp RPAR  &kp LBKT  &kp RBKT  &kp GRAVE  &kp EXCL  &kp MINUS  &kp UNDER  &kp COLON  &kp SEMI  &kp BSLH  &kp FSLH  &kp LCTRL  &lt NUMBERS SPACE  &lt NAV ENTER  &kp LSHFT
            >;
        };

        mouse_layer {
            bindings = <
&kp PSCRN  &kp C_PP  &kp C_VOL_UP  &tog DEFAULT  &kp 5  &kp 6  &kp 7  &kp 8  &kp C_PREV  &kp C_MUTE  &kp C_VOL_DN  &kp C_NEXT  &kp 1  &kp 2  &kp 3  &kp 4  &kp LCTRL  &lt NUMBERS SPACE  &lt NAV ENTER  &kp LSHFT
