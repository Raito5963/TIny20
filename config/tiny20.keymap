    keymap {
        compatible = "zmk,keymap";

        // This is a sample keymap intended to be replaced with your own

        default_layer {
            bindings = <
&kp A  &kp S  &kp U  &kp MINUS  &kp N  &kp E  &kp A  &kp I  &kp E  &kp I  &kp A  &kp O  &kp K  &kp T  &kp N  &kp S  &kp LCTRL  &lt NUMBERS ENTER  &lt NAV SPACE  &kp KP_COMMA
            >;
        };

        numbers_layer {
            bindings = <
&kp ESC  &kp LA(TAB)  &kp LC(TAB)  &kp N9  &kp N5  &kp N6  &kp N7  &kp N8  &kp TAB  &kp LA(F4)  &kp LC(F4)  &kp N0  &kp N1  &kp N2  &kp N3  &kp N4  &kp LCTRL  &lt NUMBERS SPACE  &lt NAV ENTER  &kp LSHFT
            >;
        };

        nav_layer {
            bindings = <
&kp F4  &kp F5  &kp F6  &kp DEL  &kp LEFT  &kp DOWN  &kp UP  &kp RIGHT  &kp F1  &kp F2  &kp F3  &kp BSPC  &kp HOME  &kp PG_DN  &kp PG_UP  &kp END  &kp LCTRL  &lt NUMBERS SPACE  &lt NAV ENTER  &kp LSHFT
            >;
        };

        symbols_layer {
